            android:name=".ui.view.CreateVehiculoActivity"
            android:exported="false" />
        <activity
            android:name=".ui.view.VehiculoActivity"
            android:exported="false" />
        <activity
            android:name=".ui.view.EditResourceActivity"
            android:exported="false" />
        <activity
            android:name=".ui.view.HomeActivity"
            android:exported="false" />
        <activity
            android:name=".ui.view.TourActivity"
            android:exported="false" />
        <activity
            android:name=".ui.view.SplashActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".ui.view.CreateParkingActivity"
            android:exported="true" />
        <activity
            android:name=".ui.view.ParkingListActivity"
            android:exported="true" />
        <activity
